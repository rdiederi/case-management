<h4>Dashboard</h4><br>
<div class="row text-center">
    <div class="col">
        <div class="card">
            <h5 class="card-header">Increase Cover Cases</h5>
            <div class="card-body">
                <h5 class="card-title">0{*$totalNumOfIncreaseCoverCases*}</h5>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card">
            <h5 class="card-header">Decrease Cover Cases</h5>
            <div class="card-body">
                <h5 class="card-title">{$totalNumOfDecreaseCoverCases}</h5>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card">
            <h5 class="card-header">Cancel Cover Cases</h5>
            <div class="card-body">
                <h5 class="card-title">{$totalNumOfCancelCoverCases}</h5>
            </div>
        </div>
    </div>
</div>
<br/>
<div class="row text-center">
    <div class="col">
        <div class="card">
            <h5 class="card-header">Last Created Policy</h5>
            <div class="card-body">
                <h5 class="card-title">{$lastCreatedPolicyName}</h5>
            </div>
        </div>
    </div>
</div>
